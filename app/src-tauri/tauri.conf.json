{
  "$schema": "../../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Brief Case",
  "version": "0.1.0",
  "identifier": "com.tauri.dev",
  "build": {
    "frontendDist": "../www",
    "devUrl": "http://localhost:4200",
    "beforeDevCommand": "ng serve",
    "beforeBuildCommand": "ng build"
  },
  "app": {
    "windows": [
      {
        "title": "Brief Case",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
